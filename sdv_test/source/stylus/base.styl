*
	margin 0
	padding 0

*,:after,:before,input[type=checkbox],input[type=radio]
	box-sizing border-box

html
	height 100%
	font-size $baseFontSize, false

body
	height 100%
	background-image url('data:image/png;base64,')
	background-color $bodyBg
	color $baseColor
	font-family $baseFontFamily
	min-width $minWidth px

p
	margin-bottom 15px
	line-height 18px
	&:last-child
		margin-bottom 0px

a
	color $linksColor
	&:hover
		text-decoration none

ul, ol
	margin-left 15px
	li
		margin-bottom 9px
